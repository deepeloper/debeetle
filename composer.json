{
  "name": "deepeloper/debeetle",
  "type": "project",
  "description": "PHP Debugging Tools",
  "homepage": "https://github.com/deepeloper/debeetle",
  "keywords": ["php", "debug", "debugging", "tools"],
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "deepeloper/lib-xml": "*"
  },
  "autoload": {
    "psr-4": {
      "deepeloper\\Debeetle\\": ["src"],
      "deepeloper\\Debeetle\\Plugin\\": ["plugins"],
      "deepeloper\\Debeetle\\Skin\\": ["skins"]
    }
  }
}
